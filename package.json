    "vite": "^4.4.7",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,scss,md}": [
      "yarn format:fix"
    ],
    "src/**/*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "packageManager": "yarn@3.6.1"
