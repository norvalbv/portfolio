  "devDependencies": {
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^29.5.3",
    "@types/mixpanel-browser": "^2.47.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
